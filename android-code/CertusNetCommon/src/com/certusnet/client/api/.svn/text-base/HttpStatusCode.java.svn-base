package com.certusnet.client.api;

import org.apache.http.HttpStatus;

/**
 * @author lig
 * 
 */
public interface HttpStatusCode extends HttpStatus {
	/** 请求超时 */
	public static final int REQUEST_TIMEOUT = 3005;
	/** 当前服务无效 */
	public static final int UNRESPONSIVE_HOST = 3004;
	/** * 不能识别的消息格式 */
	public static final int JSON_PARSE_ERROR = 3008;
	/** 请求异常， 网络连接失败或无法连接 */
	public static final int UNEXPECTED_ERROR = 3002;
	
	
	
	/** 智慧社区服务不存在 */
	public static final int REQUEST_SERVICE_NOT_FOUND = 3018;
	/** 智慧社区服务返回数据异常 */
	public static final int PARSE_ERROR = 3019;
	/** 智慧社区服务请求异常，没有进行网络请求 */
	public static final int Request_ERROR = 3020;
	/** 智慧社区服务 请求异常，编码错误 */
	public static final int UnsupportedEncodingException = 3021;

}